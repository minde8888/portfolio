    "yarn": "^1.22.22",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.12",
    "@types/pg": "^8.11.10",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^5.6.3"
